\n      />\r\n      <el-table-column\r\n        prop=\"customer_status\"\r\n        label=\"状态\"\r\n      >\r\n        <template slot-scope=\"scope\">\r\n          <span\r\n            v-for=\"item in status\"\r\n            v-show=\"scope.row.room_status == item.value\"\r\n            :key=\"item.type_id\"\r\n            :value=\"item.type_id\"\r\n          >\r\n            {{ item.label }}\r\n          </span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"checkin_id\"\r\n        label=\"入住单号\"\r\n      />\r\n      <el-table-column\r\n        prop=\"orders_id\"\r\n        label=\"账单编号\"\r\n      />\r\n      <el-table-column label=\"操作\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            @click=\"handleEdit(scope.row)\"\r\n          >退房</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-pagination\r\n      background\r\n      layout=\"prev, pager, next\"\r\n      :current-page=\"pageIndex\"\r\n      :page-size=\"pageSize\"\r\n      :total=\"pageTotal\"\r\n      @current-change=\"handlePageChange\"\r\n    />\r\n  </div>\r\n</template>\r\n<script>\r\nimport { getList, changStatus } from '@/api/customer'\r\nimport { changeStatusEmpty } from '@/api/room'\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: [{}],\r\n      pageIndex: 1,\r\n      pageSize: 10,\r\n      pageTotal: 0,\r\n      status: [\r\n        { value: 0, label: '退房' },\r\n        { value: 1, label: '入住' }\r\n      ]\r\n    }\r\n  },\r\n  mounted() {\r\n    getList().then(res => {\r\n      if (res.code === 200) {\r\n        this.tableData = res.data.records\r\n        this.pageTotal = res.data.total\r\n      }\r\n    })\r\n  },\r\n  methods: {\r\n    handlePageChange() {\r\n      getList(this.pageIndex, this.pageSize).then(res => {\r\n        if (res.code === 200) {\r\n          this.tableData = res.data.data\r\n          this.pageTotal = res.data.count\r\n        }\r\n      })\r\n    },\r\n    handleEdit(row) {\r\n      this.$confirm(`确定${row.customer_name}退房吗？`, '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'error'\r\n      }).then(() => {\r\n        changStatus(row.customer_id).then(res => {\r\n          if (res.code === 200) {\r\n            this.$notify({\r\n              title: res.data,\r\n              type: 'success'\r\n            })\r\n            changeStatusEmpty(row.room_id).then(res => {\r\n              this.$notify({\r\n                title: res.data,\r\n                type: 'success'\r\n              })\r\n            })\r\n            getList().then(res => {\r\n              if (res.code === 200) {\r\n                this.tableData = res.data.records\r\n                this.pageTotal = res.data.total\r\n              }\r\n            })\r\n          } else {\r\n            this.$notify({\r\n              title: res.data,\r\n              type: 'error'\r\n            })\r\n          }\r\n        })\r\n      }).catch(() => {})\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"\">\r\n\r\n</style>\r\n"]}]}