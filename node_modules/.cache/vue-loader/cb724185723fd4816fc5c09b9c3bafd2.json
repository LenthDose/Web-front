select>\r\n      </el-form-item>\r\n      <el-form :inline=\"true\" :model=\"query\">\r\n        <el-form-item label=\"客房类型\">\r\n          <el-select v-model=\"typename\" placeholder=\"请选择\">\r\n            <el-option\r\n              v-for=\"item in options\"\r\n              :key=\"item.value\"\r\n              :label=\"item.label\"\r\n              :value=\"item.value\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"状态\">\r\n          <el-select v-model=\"status\" placeholder=\"请选择\">\r\n\r\n            <el-option\r\n              v-for=\"item in options\"\r\n              :key=\"item.value\"\r\n              :label=\"item.label\"\r\n              :value=\"item.value\"\r\n            />\r\n\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n    <div>\r\n      <el-table\r\n        :data=\"tableData\"\r\n        border\r\n        style=\"width: 100%\"\r\n      >\r\n        <el-table-column\r\n          prop=\"roomid\"\r\n          label=\"客房编号\"\r\n          width=\"150\"\r\n        />\r\n        <el-table-column\r\n          prop=\"typename\"\r\n          label=\"客房类型\"\r\n          width=\"120\"\r\n        />\r\n        <el-table-column\r\n          prop=\"floortype\"\r\n          label=\"楼层类型\"\r\n          width=\"120\"\r\n        />\r\n        <el-table-column\r\n          prop=\"status\"\r\n          label=\"状态\"\r\n          width=\"120\"\r\n        />\r\n\r\n        <el-table-column\r\n          label=\"操作\"\r\n          width=\"100\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <el-button size=\"small\" @click=\"handleClick(scope.row)\">办理入住</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <el-pagination\r\n        background\r\n        layout=\"prev, pager, next\"\r\n        :current-page=\"pageIndex\"\r\n        :page-size=\"10\"\r\n        :total=\"pageTotal\"\r\n        @current-change=\"handlePageChange\"\r\n      />\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n<script>\r\nimport { getRoomInfoList } from '@/api/room'\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: {\r\n        roomid: '',\r\n        typename: '',\r\n        floortype: '',\r\n        status: ''\r\n      },\r\n      typename: [\r\n      ],\r\n      floortype: [\r\n        { value: 1, label: '标准楼层' },\r\n        { value: 2, label: '贵宾楼层' },\r\n        { value: 3, label: '桂冠楼层' },\r\n        { value: 4, label: '总统套房' }\r\n      ]\r\n    }\r\n  },\r\n  mounted() {\r\n    getRoomInfoList().then(response => {\r\n      this.tableData = response.data.data\r\n      this.pageTotal = response.data.count\r\n      console.log(this.pageTotal)\r\n    })\r\n  },\r\n  methods: {\r\n    handlePageChange(page) {\r\n      getRoomInfoList(page).then(response => {\r\n        this.tableData = response.data.data\r\n        this.pageTotal = response.data.count\r\n      })\r\n    },\r\n    handleClick(row) {\r\n      this.$router.push({\r\n        name: 'checkin',\r\n        params: {\r\n          typeid: row.typename,\r\n          roomid: row.roomid\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"css\">\r\n.roomlist{\r\n    margin: 5%;\r\n}\r\n</style>\r\n"]}]}