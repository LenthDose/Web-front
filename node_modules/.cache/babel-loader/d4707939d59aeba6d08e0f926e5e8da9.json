AA,WAAA;;AACA,UAAA,OAAA,KAAA,EAAA,IAAA,WAAA,KAAA,EAAA,EAAA;AACA,aAAA,aAAA,CAAA,WAAA,EAAA,OAAA;AACA,OAFA,MAEA,IAAA,OAAA,KAAA,EAAA,IAAA,WAAA,KAAA,EAAA,EAAA;AACA,aAAA,YAAA,CAAA,WAAA;AACA,OAFA,MAEA,IAAA,OAAA,KAAA,EAAA,IAAA,WAAA,KAAA,EAAA,EAAA;AACA,aAAA,UAAA,CAAA,OAAA;AACA;AACA,KAvDA;AAwDA,IAAA,mBAxDA,+BAwDA,GAxDA,EAwDA;AACA,WAAA,WAAA,GAAA,GAAA;AACA,UAAA,OAAA,GAAA,KAAA,OAAA,CAAA,OAAA;AACA,UAAA,WAAA,GAAA,KAAA,OAAA,CAAA,WAAA;;AACA,UAAA,OAAA,KAAA,EAAA,IAAA,WAAA,KAAA,EAAA,EAAA;AACA,aAAA,aAAA,CAAA,WAAA,EAAA,OAAA;AACA,OAFA,MAEA,IAAA,OAAA,KAAA,EAAA,IAAA,WAAA,KAAA,EAAA,EAAA;AACA,aAAA,YAAA,CAAA,WAAA;AACA,OAFA,MAEA,IAAA,OAAA,KAAA,EAAA,IAAA,WAAA,KAAA,EAAA,EAAA;AACA,aAAA,UAAA,CAAA,OAAA;AACA;AACA,KAnEA;AAoEA,IAAA,KApEA,mBAoEA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,KAtEA;AAuEA,IAAA,QAvEA,sBAuEA;AACA,WAAA,iBAAA,GAAA,KAAA;AACA,KAzEA;AA0EA,IAAA,QA1EA,sBA0EA;AAAA;;AACA,MAAA,MAAA,CAAA,KAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,UAAA,CAAA,GAAA,CAAA,IAAA,CAAA;AACA,OAFA,EAEA,KAFA,CAEA,YAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,MAAA,EAAA,WAAA;AACA,OAJA;AAKA;AAhFA;AAjCA,CAAA","sourcesContent":["<template lang=\"\">\r\n  <div class=\"roomlist\">\r\n    <div class=\"contanier-query\">\r\n      <el-form ref=\"wrapper\" :inline=\"true\" :model=\"wrapper\">\r\n        <el-form-item label=\"客房类型\" prop=\"type_id\">\r\n          <el-select v-model=\"wrapper.type_id\" placeholder=\"请选择\">\r\n            <el-option\r\n              v-for=\"item in type\"\r\n              :key=\"item.type_id\"\r\n              :label=\"item.type_name\"\r\n              :value=\"item.type_id\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"状态\" prop=\"room_status\">\r\n          <el-select v-model=\"wrapper.room_status\" placeholder=\"请选择\">\r\n            <el-option\r\n              v-for=\"item in status\"\r\n              :key=\"item.value\"\r\n              :label=\"item.label\"\r\n              :value=\"item.value\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"resetWrapper\">重置条件</el-button>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"onAdd\">添加</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n\r\n    <div>\r\n      <el-table\r\n        :data=\"tableData\"\r\n        border\r\n      >\r\n        <el-table-column\r\n          prop=\"room_id\"\r\n          label=\"客房编号\"\r\n        />\r\n        <el-table-column\r\n          prop=\"type_id\"\r\n          label=\"客房类型\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <span\r\n              v-for=\"item in type\"\r\n              v-show=\"scope.row.type_id == item.type_id\"\r\n              :key=\"item.type_id\"\r\n              :value=\"item.type_id\"\r\n            >\r\n              {{ item.type_name }}\r\n            </span>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column\r\n          prop=\"room_floor\"\r\n          label=\"楼层\"\r\n        />\r\n        <el-table-column\r\n          prop=\"room_status\"\r\n          label=\"状态\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <span\r\n              v-for=\"item in status\"\r\n              v-show=\"scope.row.room_status == item.value\"\r\n              :key=\"item.type_id\"\r\n              :value=\"item.type_id\"\r\n            >\r\n              {{ item.label }}\r\n            </span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"操作\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"danger\"\r\n              @click=\"handleDelete(scope.row)\"\r\n            >删除\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <div class=\"block\">\r\n        <el-pagination\r\n          :current-page=\"pageCurrent\"\r\n          :page-sizes=\"[10, 20, 30, 40]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"pageTotal\"\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n        />\r\n      </div>\r\n    </div>\r\n\r\n    <el-dialog :title=\"dialogTitle\" width=\"50%\" :visible.sync=\"dialogFormVisible\">\r\n      <el-form ref=\"room\" :model=\"room\">\r\n        <el-form-item label=\"客房类型\">\r\n          <el-select v-model=\"room.type_id\" placeholder=\"请选择\">\r\n            <el-option\r\n              v-for=\"item in type\"\r\n              :key=\"item.type_id\"\r\n              :label=\"item.type_name\"\r\n              :value=\"item.type_id\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"楼层\">\r\n          <el-input v-model=\"room.room_floor\" />\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"onSubmit\">确定</el-button>\r\n          <el-button type=\"primary\" @click=\"onCancel\">取消</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { addOne, deleteById, getList, getListByStatus, getListByType, getListByWarpper } from '@/api/room'\r\nimport { getTypeOtherList } from '@/api/type'\r\nimport { showNotify } from '@/api/notifty'\r\nexport default {\r\n  data() {\r\n    return {\r\n      pageCurrent: 1,\r\n      pageSize: 10,\r\n      pageTotal: 0,\r\n      status: [\r\n        { value: 0, label: '空闲' },\r\n        { value: 1, label: '已入住' }\r\n      ],\r\n      wrapper: {\r\n        type_id: '',\r\n        room_status: ''\r\n      },\r\n      type: [],\r\n      tableData: [],\r\n      dialogTitle: '添加',\r\n      dialogFormVisible: false,\r\n      room: {}\r\n    }\r\n  },\r\n  watch: {\r\n    'wrapper.room_status'(val) {\r\n      this.ListByStatus(val)\r\n    },\r\n    'wrapper.type_id'(val) {\r\n      this.ListByType(val)\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getTypeList()\r\n    this.List()\r\n  },\r\n  methods: {\r\n    getTypeList() {\r\n      getTypeOtherList().then(res => {\r\n        this.type = res.data\r\n      })\r\n    },\r\n    List() {\r\n      getList(this.pageCurrent, this.pageSize).then(res => {\r\n        this.tableData = res.data.records\r\n        this.pageTotal = res.data.total\r\n      })\r\n    },\r\n    ListByStatus(val) {\r\n      getListByStatus(this.pageIndex, this.pageSize, val).then(res => {\r\n        this.tableData = res.data.records\r\n        this.pageTotal = res.data.total\r\n      })\r\n    },\r\n    ListByType(val) {\r\n      getListByType(this.pageIndex, this.pageSize, val).then(res => {\r\n        this.tableData = res.data.records\r\n        this.pageTotal = res.data.total\r\n      })\r\n    },\r\n    ListByWrapper(status, type) {\r\n      getListByWarpper(this.pageIndex, this.pageSize, status, type).then(res => {\r\n        this.tableData = res.data.records\r\n        this.pageTotal = res.data.total\r\n      })\r\n    },\r\n    resetWrapper() {\r\n      this.$ref['wrapper'].resetFields()\r\n    },\r\n    handleDelete(row) {\r\n      this.$confirm(`确定删除${row.room_id}吗？`, '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'error'\r\n      }).then(() => {\r\n        deleteById(row.room_id).then(() => {\r\n          this.List()\r\n        })\r\n      })\r\n    },\r\n    handleSizeChange(val) {\r\n      this.pageSize = val\r\n      const type_id = this.wrapper.type_id\r\n      const room_status = this.wrapper.room_status\r\n      if (type_id !== '' && room_status !== '') {\r\n        this.ListByWrapper(room_status, type_id)\r\n      } else if (type_id === '' && room_status !== '') {\r\n        this.ListByStatus(room_status)\r\n      } else if (type_id !== '' && room_status === '') {\r\n        this.ListByType(type_id)\r\n      }\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.pageCurrent = val\r\n      const type_id = this.wrapper.type_id\r\n      const room_status = this.wrapper.room_status\r\n      if (type_id !== '' && room_status !== '') {\r\n        this.ListByWrapper(room_status, type_id)\r\n      } else if (type_id === '' && room_status !== '') {\r\n        this.ListByStatus(room_status)\r\n      } else if (type_id !== '' && room_status === '') {\r\n        this.ListByType(type_id)\r\n      }\r\n    },\r\n    onAdd() {\r\n      this.dialogFormVisible = true\r\n    },\r\n    onCancel() {\r\n      this.dialogFormVisible = false\r\n    },\r\n    onSubmit() {\r\n      addOne(this.room).then(res => {\r\n        showNotify(res.data)\r\n      }).catch(() => {\r\n        this.$ref['room'].resetFields()\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"css\">\r\n.roomlist{\r\n    margin: 5%;\r\n}\r\n .block{\r\n  position: relative;\r\n  margin-top: 5%;\r\n  left:30%;\r\n}\r\n.el-table {\r\n   box-shadow: 0 2px 4px rgba (0, 0, 0, .12), 0 0 6px rgba (0, 0, 0, .04);\r\n   border-radius: 4px\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/back"}]}