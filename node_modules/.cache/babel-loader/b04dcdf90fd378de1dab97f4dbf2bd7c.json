AAA,OAAA,CAAA,KAAA,WAAA,EAAA,KAAA,QAAA,EAAA,GAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAHA;AAIA,KANA;AAOA,IAAA,gBAPA,4BAOA,GAPA,EAOA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,WAAA,IAAA;AACA,KAVA;AAWA,IAAA,mBAXA,+BAWA,GAXA,EAWA;AACA,WAAA,WAAA,GAAA,GAAA;AACA,WAAA,IAAA;AACA,KAdA;AAeA,IAAA,UAfA,sBAeA,GAfA,EAeA;AAAA;;AACA,WAAA,QAAA,uBAAA,GAAA,CAAA,aAAA,+BAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,WAAA,CAAA,GAAA,CAAA,WAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,iBAAA,CAAA,GAAA,CAAA,OAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,UAAA,CAAA,GAAA,CAAA,IAAA,CAAA;AACA,WAFA;AAGA,UAAA,UAAA,CAAA,GAAA,CAAA,UAAA,CAAA;AACA,SALA;;AAMA,QAAA,MAAA,CAAA,IAAA;AACA,OAZA,EAYA,KAZA,CAYA,YAAA;AACA,QAAA,WAAA,CAAA,MAAA,CAAA;AACA,OAdA;AAeA;AA/BA;AAtBA,CAAA","sourcesContent":["<template lang=\"\">\r\n  <div class=\"container\">\r\n    <el-select v-model=\"customer_status\" placeholder=\"请选择\">\r\n      <el-option\r\n        v-for=\"item in status\"\r\n        :key=\"item.value\"\r\n        :label=\"item.label\"\r\n        :value=\"item.value\"\r\n      />\r\n    </el-select>\r\n    <el-table\r\n      :data=\"tableData\"\r\n      border\r\n      style=\"width: 85%\"\r\n    >\r\n      <el-table-column\r\n        :show-overflow-tooltip=\"true\"\r\n        prop=\"customer_id\"\r\n        label=\"客户编号\"\r\n      />\r\n      <el-table-column\r\n        prop=\"customer_name\"\r\n        label=\"客户姓名\"\r\n      />\r\n      <el-table-column\r\n        :show-overflow-tooltip=\"true\"\r\n        prop=\"customer_idcard\"\r\n        label=\"身份证号\"\r\n      />\r\n      <el-table-column\r\n        :show-overflow-tooltip=\"true\"\r\n        prop=\"room_id\"\r\n        label=\"客房编号\"\r\n      />\r\n      <el-table-column\r\n        prop=\"customer_status\"\r\n        label=\"状态\"\r\n      >\r\n        <template slot-scope=\"scope\">\r\n          <el-tag\r\n            v-for=\"item in status\"\r\n            v-show=\"scope.row.customer_status == item.value\"\r\n            :key=\"item.value\"\r\n            :value=\"item.value\"\r\n          >\r\n            {{ item.label }}\r\n          </el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        :show-overflow-tooltip=\"true\"\r\n        prop=\"checkin_id\"\r\n        label=\"入住单号\"\r\n      />\r\n      <el-table-column\r\n        :show-overflow-tooltip=\"true\"\r\n        prop=\"orders_id\"\r\n        label=\"账单编号\"\r\n      />\r\n      <el-table-column label=\"操作\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            v-if=\"scope.row.customer_status === 1 \"\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"handleEdit(scope.row)\"\r\n          >退房</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div class=\"block\">\r\n      <el-pagination\r\n        :current-page=\"pageCurrent\"\r\n        :page-sizes=\"[10, 20, 30, 40]\"\r\n        :page-size=\"pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"pageTotal\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n      />\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { getList, changStatus } from '@/api/customer'\r\nimport { changeStatusEmpty } from '@/api/room'\r\nimport { showNotify, showMessage } from '@/api/notifty'\r\nimport { deleteById } from '@/api/checkin'\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: [{}],\r\n      pageCurrent: 1,\r\n      pageSize: 10,\r\n      pageTotal: 0,\r\n      status: [\r\n        { value: 0, label: '退房' },\r\n        { value: 1, label: '入住' }\r\n      ],\r\n      customer_status: 1\r\n    }\r\n  },\r\n  watch: {\r\n    'customer_status'(val) {\r\n      this.List(val)\r\n    }\r\n  },\r\n  mounted() {\r\n    this.List(this.customer_status)\r\n  },\r\n  methods: {\r\n    List(val) {\r\n      getList(this.pageCurrent, this.pageSize, val).then(res => {\r\n        this.tableData = res.data.records\r\n        this.pageTotal = res.data.total\r\n      })\r\n    },\r\n    handleSizeChange(val) {\r\n      this.pageSize = val\r\n      this.List()\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.pageCurrent = val\r\n      this.List()\r\n    },\r\n    handleEdit(row) {\r\n      this.$confirm(`确定${row.customer_name}退房吗？`, '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'error'\r\n      }).then(() => {\r\n        changStatus(row.customer_id).then(res => {\r\n          changeStatusEmpty(row.room_id).then(res => {\r\n            showNotify(res.data)\r\n          })\r\n          deleteById(row.checkin_id)\r\n        })\r\n        this.List()\r\n      }).catch(() => {\r\n        showMessage('操作失败')\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"css\">\r\n .container {\r\n    margin-left:13%;\r\n    margin-top: 5%;\r\n }\r\n.el-table {\r\n  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n  border-radius: 4px\r\n}\r\n  .block{\r\n  position: relative;\r\n  margin-top: 5%;\r\n  left:25%;\r\n}\r\n.el-select{\r\n  margin-bottom: 2%;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/front"}]}