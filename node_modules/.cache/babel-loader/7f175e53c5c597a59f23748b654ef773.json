nfo.typeid\" placeholder=\"请选择\" @change=\"getTypeName(roominfo.typeid)\">\r\n          <el-option\r\n            v-for=\"item in typenames\"\r\n            :key=\"item.typeid\"\r\n            :label=\"item.typename\"\r\n            :value=\"item.typeid\"\r\n          />\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"楼层\">\r\n        <el-select v-model=\"roominfo.floorid\" placeholder=\"请选择\">\r\n          <el-option\r\n            v-for=\"item in floorname\"\r\n            :key=\"item.floorid\"\r\n            :label=\"item.floorname\"\r\n            :value=\"item.floorid\"\r\n          />\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"客房编号\">\r\n        <el-input v-model=\"roominfo.roomid\" style=\"width: 25%;\" />\r\n      </el-form-item>\r\n      <el-form-item label=\"状态\">\r\n        <el-checkbox-group v-model=\"status\">\r\n          <el-checkbox label=\"已入住\" />\r\n          <el-checkbox label=\"空闲\" />\r\n        </el-checkbox-group>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"onSubmit\">立即创建</el-button>\r\n        <el-button>取消</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { addRoomInfo, getRoomType, getFloorName } from '@/api/room'\r\nexport default {\r\n  data() {\r\n    return {\r\n      floortypes: [\r\n        { value: 1, label: '标准楼层' },\r\n        { value: 2, label: '贵宾楼层' },\r\n        { value: 3, label: '桂冠楼层' },\r\n        { value: 4, label: '总统套房' }\r\n      ],\r\n      status: [],\r\n      typenames: [{ typeid: '', typename: '' }],\r\n      floorname: [],\r\n      roominfo: {\r\n        floortype: '',\r\n        typename: '',\r\n        status: '',\r\n        floorid: '',\r\n        roomid: '',\r\n        typeid: ''\r\n\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n\r\n    getType(val) {\r\n      var type = ''\r\n      if (val) {\r\n        let obj = {}\r\n        obj = this.floortypes.find((item) => {\r\n          return item.value === val\r\n        })\r\n        type = obj.label\r\n        this.roominfo.floortype = type\r\n      }\r\n      getRoomType(type).then(response => {\r\n        this.typenames = response.data\r\n      })\r\n      getFloorName(type).then(res => {\r\n        this.floorname = res.data\r\n      })\r\n    },\r\n    getTypeName(val) {\r\n      var type = ''\r\n      if (val) {\r\n        let obj = {}\r\n        obj = this.typenames.find((item) => {\r\n          return item.typeid === val\r\n        })\r\n        type = obj.typename\r\n        this.roominfo.typename = type\r\n      }\r\n    },\r\n    onSubmit() {\r\n      this.roominfo.status = this.status[0]\r\n      console.log(this.roominfo)\r\n      addRoomInfo(this.roominfo).then(res => {\r\n        if (res.code === 200) {\r\n          this.$notify({\r\n            title: res.data,\r\n            type: 'success'\r\n          })\r\n        } else {\r\n          this.$$notify({\r\n            title: res.data,\r\n            type: 'error'\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"css\">\r\n\r\n.roominfo{\r\n    margin: 5%;\r\n    box-shadow: 0 2px 12px 0 rgba (0, 0, 0, 0.1)\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/back"}]}