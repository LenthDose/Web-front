,\r\n    handleClick(row) {\r\n      this.$router.push({\r\n        name: 'checkin',\r\n        params: {\r\n          typeid: row.typename,\r\n          roomid: row.roomid\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"css\">\r\n.roomlist{\r\n    margin: 5%;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/room"}]}