    <el-button\r\n              size=\"mini\"\r\n              @click=\"onSubmit(scope.row)\"\r\n            >提交\r\n            </el-button>\r\n          </div>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <el-dialog :title=\"dialogTitle\" width=\"50%\" :visible.sync=\"dialogFormVisible\">\r\n      <el-table\r\n        :data=\"roomData\"\r\n        border\r\n      >\r\n        <el-table-column\r\n          prop=\"room_id\"\r\n          label=\"客房编号\"\r\n        />\r\n        <el-table-column\r\n          prop=\"room_floor\"\r\n          label=\"楼层\"\r\n        />\r\n        <el-table-column\r\n          prop=\"room_status\"\r\n          label=\"状态\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <span v-if=\"scope.row.room_status == 0\">\r\n              空闲\r\n            </span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"操作\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"danger\"\r\n              @click=\"handleChoose(scope.row)\"\r\n            >选择\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapGetters } from 'vuex'\r\nimport { getEmptyRoomWithType } from '@/api/room'\r\nimport { getBookNotCommit, chooseRoom, update } from '@/api/book'\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: [{}],\r\n      roomData: [{}],\r\n      status: [\r\n        { value: 0, label: '空闲' },\r\n        { value: 1, label: '已入住' }\r\n      ],\r\n      dialogFormVisible: false,\r\n      dialogTitle: '选择',\r\n      book_id: ''\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapGetters([\r\n      'id'\r\n    ])\r\n  },\r\n  mounted() {\r\n    getBookNotCommit().then(res => {\r\n      if (res.code === 200) {\r\n        this.tableData = res.data\r\n      } else {\r\n        this.$notify({\r\n          title: res.data,\r\n          type: 'error'\r\n        })\r\n      }\r\n    })\r\n  },\r\n  methods: {\r\n    handleEdit(row) {\r\n      this.dialogFormVisible = true\r\n      this.book_id = row.book_id\r\n      getEmptyRoomWithType(row.type_id).then(res => {\r\n        if (res.code === 200) {\r\n          this.roomData = res.data\r\n        } else {\r\n          this.$notify({\r\n            title: res.data,\r\n            type: 'error'\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleChoose(row) {\r\n      chooseRoom(this.book_id, row.room_id).then(res => {\r\n        if (res.code === 200) {\r\n          this.$notify({\r\n            title: res.data,\r\n            type: 'success'\r\n          })\r\n          getBookNotCommit().then(res => {\r\n            if (res.code === 200) {\r\n              this.tableData = res.data\r\n            } else {\r\n              this.$notify({\r\n                title: res.data,\r\n                type: 'error'\r\n              })\r\n            }\r\n          })\r\n        } else {\r\n          this.$notify({\r\n            title: res.data,\r\n            type: 'error'\r\n          })\r\n        }\r\n      })\r\n\r\n      this.dialogFormVisible = false\r\n    },\r\n    onSubmit(row) {\r\n      this.$confirm('确定无误提交吗？', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'info'\r\n      }).then(() => {\r\n        update(row.book_id, this.id).then(res => {\r\n          if (res.code === 200) {\r\n            this.$notify({\r\n              title: res.data,\r\n              type: 'success'\r\n            })\r\n            getBookNotCommit().then(res => {\r\n              if (res.code === 200) {\r\n                this.tableData = res.data\r\n              } else {\r\n                this.$notify({\r\n                  title: res.data,\r\n                  type: 'error'\r\n                })\r\n              }\r\n            })\r\n          } else {\r\n            this.$notify({\r\n              title: res.data,\r\n              type: 'error'\r\n            })\r\n          }\r\n        })\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: 'info',\r\n          message: '已取消提交'\r\n        })\r\n      })\r\n    }\r\n\r\n  }\r\n}\r\n</script>\r\n<style lang=\"\">\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/back"}]}